<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="#122770" />
            <size
                android:height="60dp"
                android:width="60dp" />
        </shape>
    </item>
    <item android:state_activated="true">
        <shape android:shape="oval">
            <solid android:color="#c79312" />
            <size
                android:height="60dp"
                android:width="60dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#c79312" />
            <size
                android:height="60dp"
                android:width="60dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="#122770" />
            <size
                android:height="60dp"
                android:width="60dp" />
        </shape>
    </item>
</selector>
